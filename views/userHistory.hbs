<div class="row">
    <div class="col-md-12">

        <h2>{{user.userName}} ( {{user.email}} ) History</h2>
        <hr />

        <div class="table-responsive">
            <table class="table ">
                <thead>
                    <tr>
                        <th>Login Date/Time</th>
                        <th>Client Information</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each user.loginHistory}}
                    <tr>
                        <td>{{this.dateTime}}</td>
                        <td>{{this.userAgent}}</td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>

    </div>